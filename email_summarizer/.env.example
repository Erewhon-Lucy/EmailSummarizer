EXCHANGE_EMAIL=your_exchange_email@example.com
# Exchange Server Config
EXCHANGE_USERNAME=your_domain\\your_username
EXCHANGE_PASSWORD=your_password
EXCHANGE_SERVER=your_exchange_server

# Azure OpenAI API

AZURE_OPENAI_API_ENDPOINT = https://your-azure-openai-endpoint/
AZURE_OPENAI_API_KEY = your_azure_openai_api_key
AZURE_OPENAI_DEEP_THINKING_DEPLOYMENT_NAME = your_deep_thinking_deployment_name
AZURE_OPENAI_QUICK_THINKING_DEPLOYMENT_NAME = your_quick_thinking_deployment_name
AZURE_OPENAI_API_VERSION = 2024-12-01-preview

# Celery Config
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1
CELERY_TIMEZONE=Asia/Shanghai

# Report Config
REPORT_RECIPIENT=report_recipient@example.com
REPORT_TIME=08:00  # 24h format, daily summary time

# Other
LANGGRAPH_CONFIG=  # Optional: path or settings for langgraph
